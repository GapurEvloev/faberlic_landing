            .header__col.header__col--right
                //- Вход / Регистрация
                ul.header__list.header__list--sign
                    li.header__list-item.header__list-item--signin
                        a(href="#").header__list-link
                            +icon("signin").header__list-icon
                            span Вход
                    li.header__list-item.header__list-item--signup
                        a(href="#").header__list-link
                            span Регистрация
                //- Каталог / Корзина
                ul.header__list.header__list--basket
                    li.header__list-item.header__list-item--favorite
                        a(href="#" title="Избранное").header__list-link
                            +icon("favorite").header__list-icon
                    li.header__list-item.header__list-item--catatlog
                        a(href="#" title="Каталог").header__list-link
                            +icon("catalog").header__list-icon
                    li.header__list-item.header__list-item--basket
                        a(href="#" title="Корзина").header__list-link 
                            +icon("basket").header__list-icon


        .header__bottom.header__row
            nav.header__nav.nav
                ul.nav__list
                    li.nav__item
                        a(href="#").nav__link.nav__link--new НОВИНКИ
                    li.nav__item
                        a(href="#").nav__link КОСМЕТИКА
                    li.nav__item
                        a(href="#").nav__link ПАРФЮМЕРИЯ
                    li.nav__item
                        a(href="#").nav__link ОДЕЖДА И АКСЕССУАРЫ
                    li.nav__item
                        a(href="#").nav__link ЗДОРОВЬЕ
                    li.nav__item
                        a(href="#").nav__link ДОМ
                    li.nav__item
                        a(href="#").nav__link ДЕТЯМ
                    li.nav__item
                        a(href="#").nav__link МУЖЧИНАМ
                    li.nav__item
                        a(href="#").nav__link.nav__link--mariana ДЛЯ БИЗНЕСА
                    li.nav__item
                        a(href="#").nav__link.nav__link--stocks АКЦИИ
            
            .header__search.search
                form(action="")
                    input(type="text").search__field.field
                    button(type="submit").search__button
                        +icon("search").search__icon

                                                                                         